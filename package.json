{
  "name": "vue-manage",
  "version": "0.0.1",
  "description": "基础后台管理系统",
  "author": "yp",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build:stage": "vue-cli-service build --mode staging",
    "build:prod": "vue-cli-service build --mode production",
    "lint": "eslint --ext .js,.vue src",
    "fix": "eslint --fix --ext .js,.vue src",
    "svgo": "svgo -f src/base/icon/svg --config=src/base/icon/svgo.yml"
  },
  "dependencies": {
    "axios": "0.18.0",
    "dayjs": "1.8.19",
    "element-ui": "2.13.0",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "vue": "2.6.10",
    "vue-router": "3.1.3",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/register": "7.6.2",
    "@vue/cli-plugin-babel": "3.6.0",
    "@vue/cli-plugin-eslint": "3.11.0",
    "@vue/cli-service": "3.11.0",
    "@vue/eslint-config-standard": "4.0.0",
    "autoprefixer": "9.5.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "23.6.0",
    "eslint": "6.4.0",
    "eslint-plugin-vue": "5.2.3",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "node-sass": "4.12.0",
    "prettier": "1.19.1",
    "sass-loader": "8.0.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "0.7.2",
    "svg-sprite-loader": "4.2.1",
    "svgo": "1.3.2",
    "vue-template-compiler": "2.6.10"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix --ext .js,.vue src",
      "git add"
    ]
  }
}
